{
  "name": "mui-icons-bootstrap",
  "version": "1.11.0",
  "description": "Bootstrap icons as MUI SvgIcon components",
  "keywords": [
    "react",
    "component",
    "icon",
    "mui",
    "svg",
    "material-ui",
    "bootstrap"
  ],
  "homepage": "https://github.com/guoyunhe/mui-icons-bootstrap#readme",
  "bugs": {
    "url": "https://github.com/guoyunhe/mui-icons-bootstrap/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guoyunhe/mui-icons-bootstrap.git"
  },
  "funding": "https://github.com/sponsors/guoyunhe",
  "license": "ISC",
  "author": {
    "name": "Guo Yunhe",
    "email": "i@guoyunhe.me",
    "url": "https://guoyunhe.me/"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "node generate.mjs && react-lib-scripts build",
    "deploy": "gh-pages -d build",
    "format": "react-lib-scripts format",
    "lint": "react-lib-scripts lint",
    "start": "react-lib-scripts start",
    "test": "react-lib-scripts test",
    "watch": "react-lib-scripts watch"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true
  },
  "eslintConfig": {
    "extends": "@guoyunhe/react-lib-scripts"
  },
  "eslintIgnore": [
    "node_modules",
    "coverage",
    "dist"
  ],
  "stylelint": {
    "extends": [
      "stylelint-config-rive"
    ]
  },
  "jest": {
    "preset": "@guoyunhe/react-lib-scripts"
  },
  "devDependencies": {
    "@guoyunhe/react-lib-scripts": "^1.4.0",
    "@mui/material": "^5.14.16",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@tsconfig/strictest": "^2.0.2",
    "@tsconfig/vite-react": "^2.0.1",
    "@types/jest": "^29.5.7",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.34",
    "@types/react-dom": "^18.2.14",
    "bootstrap-icons": "^1.11.1",
    "gh-pages": "^5.0.0",
    "pascal-case": "^3.1.2",
    "react": "^18.2.0",
    "react-doc-ui": "^1.0.1",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "@mui/material": "^5.0.0",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0"
  }
}
